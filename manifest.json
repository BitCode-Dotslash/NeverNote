{
    "name": "NeverNote",
    "description": "A Google Chrome extension!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js":["/src/script/jquery.min.js","/src/script/content.js"]
      }
    ],
    "action": {
      "default_popup": "/src/popup/popup.html",
      "default_icon": {
      "48": "/images/icon.png",
      "128": "/images/icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }