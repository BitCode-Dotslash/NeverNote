{
    "name": "NeverNote",
    "description": "A Google Chrome extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "storage"
    ],
    "icons": {
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js":["/src/content/jquery.min.js","/src/content/content.js"]
      }
    ],
    "action": {
      "default_popup": "/src/popup/popup.html",
      "default_icon": {
      "48": "/images/icon.png",
      "128": "/images/icon.png"
      }
    },
    "background": {
      "service_worker": "/src/background.js"
    },
    "web_accessible_resources": [
      {
        "resources": [ "/src/content/content.html"],
        "matches": ["https://*/*"]
      }
    ]
  }